{
  "name": "steadyflow",
  "version": "0.1.0",
  "type": "module",
  "license": "GPL-3.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./types/steadyflow.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./types/steadyflow.d.ts"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "tsup src/index.js --format cjs,esm",
    "dev": "tsup src/index.js --format cjs,esm --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "immutable": "^5.1.4",
    "winston": "^3.18.3"
  }
}