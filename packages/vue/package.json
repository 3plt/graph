{
  "name": "@3plate/graph-vue",
  "version": "0.1.11",
  "type": "module",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/3plt/graph",
    "directory": "packages/vue"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@3plate/graph-core": "workspace:*"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external vue",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external vue"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vue": "^3.5.25"
  }
}