{
  "compilerOptions": {
    "target": "ES2024",
    "module": "ESNext",
    "lib": [
      "ES2024",
      "DOM"
    ],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true
  }
}